{
  "expo": {
    "name": "LearnLocal",
    "slug": "learn-local-native-new",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/learl.png",
    "scheme": "learnlocalnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/learl.png",
        "backgroundColor": "#4B2ACF"
      },
      "edgeToEdgeEnabled": true,
      "statusBarColor": "#3D0DBD",
      "navigationBarColor": "#3D0DBD",
      "statusBarStyle": "light",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.haseeb241.learnlocalnative",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBOnKo1kwJz0eVMUotPndQGXR5QjWcyIX0"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/learl.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/learl.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#3D0DBD"
        }
      ],
      "expo-asset",
      "expo-image-picker",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow LearnLocal to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3f089b9f-2236-446c-8ec6-dc92bf40a414"
      }
    }
  }
}
